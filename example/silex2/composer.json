{
    "name": "fabpot/silex-skeleton",
    "description": "A pre-configured skeleton for the Silex microframework",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "bugsnag/bugsnag-silex": "^2.1",
        "silex/silex": "^2.0",
        "silex/web-profiler": "^2.0",
        "symfony/asset": "3.0.*",
        "symfony/browser-kit": "3.0.*",
        "symfony/class-loader": "3.0.*",
        "symfony/config": "3.0.*",
        "symfony/console": "3.0.*",
        "symfony/css-selector": "3.0.*",
        "symfony/debug": "3.0.*",
        "symfony/finder": "3.0.*",
        "symfony/form": "3.0.*",
        "symfony/monolog-bridge": "3.0.*",
        "symfony/process": "3.0.*",
        "symfony/security": "3.0.*",
        "symfony/translation": "3.0.*",
        "symfony/twig-bridge": "3.0.*",
        "symfony/validator": "3.0.*"
    },
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "scripts": {
        "run": [
            "echo 'Started web server on http://localhost:8888'",
            "php -S localhost:8888 -t web web/index_dev.php"
        ]
    }
}
